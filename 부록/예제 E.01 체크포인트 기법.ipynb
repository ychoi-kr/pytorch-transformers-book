    "    w = z * y\n",
    "    return w\n",
    "\n",
    "x = torch.randn(1000, 1000, requires_grad=True)\n",
    "y = torch.randn(1000, 1000, requires_grad=True)\n",
    "result = cp.checkpoint(function, x, y)"
