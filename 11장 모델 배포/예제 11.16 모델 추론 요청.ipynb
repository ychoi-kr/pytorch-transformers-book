    "import json\n",
    "import requests\n",
    "\n",
    "\n",
    "url = \"http://127.0.0.1:8000/predict\"\n",
    "headers = {\"content-type\": \"application/json\"}\n",
    "\n",
    "response = requests.post(\n",
    "    url=url,\n",
    "    headers=headers,\n",
    "    data=json.dumps({\"text\": \"정말 재미 있어요!\"})\n",
    ")\n",
    "\n",
    "print(response.status_code)\n",
    "print(response.json())"
   ]
  }
